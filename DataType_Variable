public class DataTypeExample {

    public static void main(String[] args) {
        String name = "John";
        System.out.println(name);

        int myNum = 15;
        System.out.println(myNum);

        int anotherNum = 5;
        float myFloatNum = 5.99f;
        char myLetter = 'D';
        boolean myBool = true;
        String myText = "Hello";

        // Printing out the variables
        System.out.println("Value of anotherNum: " + anotherNum);
        System.out.println("Value of myFloatNum: " + myFloatNum);
        System.out.println("Value of myLetter: " + myLetter);
        System.out.println("Value of myBool: " + myBool);
        System.out.println("Value of myText: " + myText);
    }
}
